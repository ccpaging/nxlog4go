{
    "version": "2.0.0",
    "filters": [
        {
            "enabled": "true",
            "tag": "stdout",
            "type": "stdout",
            "dsn": "",
            "level": "DEBUG",
            "properties": [
                {
                    "name": "format",
                    "value": "[%D %T] [%L] (%S) %M"
                },
                {
                    "name": "color",
                    "value": "true"
                }
            ]
        },
        {
            "enabled": "true",
            "tag": "loglog",
            "type": "loglog",
            "dsn": "",
            "level": "DEBUG",
            "properties": [
                {
                    "name": "format",
                    "value": "[%D %T] [%L] (%S) %M"
                }
            ]
        },
        {
            "enabled": "true",
            "tag": "console",
            "type": "console",
            "dsn": "",
            "level": "DEBUG",
            "properties": [
                {
                    "name": "color",
                    "value": "true"
                }
            ]
        },
        {
            "enabled": "true",
            "tag": "file",
            "type": "file",
            "dsn": "_test.log",
            "level": "FINEST",
            "properties": [
                {
                    "name": "format",
                    "value": "[%D %T] [%L] (%S) %M"
                },
                {
                    "name": "rotate",
                    "value": "-1"
                },
                {
                    "name": "maxsize",
                    "value": "5k"
                },
                {
                    "name": "maxlines",
                    "value": "0K"
                },
                {
                    "name": "cycle",
                    "value": "86400s"
                },
                {
                    "name": "clock",
                    "value": "0s"
                }
            ]
        },
        {
            "enabled": "true",
            "tag": "xmlog",
            "type": "xml",
            "dsn": "_trace.xml",
            "level": "TRACE",
            "properties": [
                {
                    "name": "rotate",
                    "value": "1"
                },
                {
                    "name": "maxsize",
                    "value": "100M"
                },
                {
                    "name": "maxrecords",
                    "value": "6K"
                },
                {
                    "name": "cycle",
                    "value": "5s"
                },
                {
                    "name": "clock",
                    "value": "0s"
                }
            ]
        },
        {
            "enabled": "false",
            "tag": "donotopen",
            "type": "socket",
            "dsn": "udp://192.168.1.255:12124",
            "level": "FINEST",
            "properties": null
        }
    ]
}