{
    "version": "",
    "filters": [
        {
            "enabled": "true",
            "tag": "stdout",
            "type": "stdout",
            "dsn": "",
            "level": "DEBUG",
            "properties": [
                {
                    "name": "format",
                    "value": "[%D %T] [%L] (%S) %M"
                },
                {
                    "name": "color",
                    "value": "true"
                }
            ]
        },
        {
            "enabled": "true",
            "tag": "loglog",
            "type": "loglog",
            "dsn": "",
            "level": "DEBUG",
            "properties": [
                {
                    "name": "format",
                    "value": "[%D %T] [%L] (%S) %M"
                }
            ]
        },
        {
            "enabled": "true",
            "tag": "console",
            "type": "console",
            "dsn": "",
            "level": "DEBUG",
            "properties": [
                {
                    "name": "color",
                    "value": "true"
                }
            ]
        },
        {
            "enabled": "true",
            "tag": "file",
            "type": "file",
            "dsn": "",
            "level": "FINEST",
            "properties": [
                {
                    "name": "filename",
                    "value": "_test.log"
                },
                {
                    "name": "format",
                    "value": "[%D %T] [%L] (%S) %M"
                },
                {
                    "name": "rotate",
                    "value": "false"
                },
                {
                    "name": "maxsize",
                    "value": "0M"
                },
                {
                    "name": "maxlines",
                    "value": "0K"
                },
                {
                    "name": "daily",
                    "value": "true"
                }
            ]
        },
        {
            "enabled": "true",
            "tag": "xmlog",
            "type": "xml",
            "dsn": "",
            "level": "TRACE",
            "properties": [
                {
                    "name": "filename",
                    "value": "_trace.xml"
                },
                {
                    "name": "rotate",
                    "value": "true"
                },
                {
                    "name": "maxsize",
                    "value": "100M"
                },
                {
                    "name": "maxrecords",
                    "value": "6K"
                },
                {
                    "name": "daily",
                    "value": "false"
                }
            ]
        },
        {
            "enabled": "false",
            "tag": "donotopen",
            "type": "socket",
            "dsn": "",
            "level": "FINEST",
            "properties": [
                {
                    "name": "endpoint",
                    "value": "192.168.1.255:12124"
                },
                {
                    "name": "protocol",
                    "value": "udp"
                }
            ]
        }
    ]
}